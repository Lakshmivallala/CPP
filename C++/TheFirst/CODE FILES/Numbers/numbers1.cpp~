#include <iostream>

using namespace std;

int main() //function
{
    int n=5;
    double d=5.5;
    cout<<-534.544<<"\n"<<8*8<<endl;
    cout<<10%3<<endl;
    cout<<n<<endl;
    n++;
    cout<<n<<endl;
    n+=2;
    cout<<n<<endl;
    cout<<5.5+7<<" Output in decimal form"<<endl;
    cout<<10/3<<"\n"<<15/4.0<<endl; //10/3 results in integer 3 since 10 and 3 are integers
    //If we make one of them (say 10=10.0), then 10.0/3=3.33

    return 0;
}

