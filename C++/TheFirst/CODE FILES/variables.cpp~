
    
#include <iostream>

using namespace std;

int main() //function
{
    cout << "Hello world!" << endl; //cout- console out, endl- end line
    string Name="Kaira";
    int age;
    age=96;
    cout<<"There was a woman named "<< Name <<endl;
    cout<<"She was "<<age<<" years old"<<endl;

    Name="Ciara";
    cout<<"Though she liked her name "<<Name<<", she was worried she'd die soon"<<endl;
}

